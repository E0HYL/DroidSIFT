package mySoot;

import java.util.Vector;

class SummaryPair{
	
	private Vector<String> dest;
	private Vector<String> source;
	
	public void addDest(String d){
		this.dest.add(d);
	}
	
	public void addSource(String s){
		this.source.add(s);
	}
	
	public Vector<String> getDest(){
		return this.dest;
	}
	
	public Vector<String> getSource(){
		return this.source;
	}
	
	public SummaryPair(){
		this.dest = new Vector<String>();
		this.source = new Vector<String>();
	}
}
